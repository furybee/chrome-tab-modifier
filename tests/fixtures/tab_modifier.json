{
    "rules": [
        {
            "name": "Local dev",
            "detection": "CONTAINS",
            "url_fragment": ".local",
            "tab": {
                "title": "[DEV] {title}",
                "icon": null,
                "pinned": false,
                "protected": false,
                "unique": false,
                "muted": false,
                "url_matcher": null
            }
        },
        {
            "name": "Production",
            "detection": "CONTAINS",
            "url_fragment": "domain.com",
            "tab": {
                "title": "[PROD] {title}",
                "icon": null,
                "pinned": false,
                "protected": false,
                "unique": false,
                "muted": false,
                "url_matcher": null
            }
        },
        {
            "name": "Youtube",
            "detection": "CONTAINS",
            "url_fragment": "youtube.com",
            "tab": {
                "title": null,
                "icon": "https://www.google.com/favicon.ico",
                "pinned": true,
                "protected": false,
                "unique": false,
                "muted": false,
                "url_matcher": null
            }
        },
        {
            "name": "Twitter",
            "detection": "CONTAINS",
            "url_fragment": "twitter.com",
            "tab": {
                "title": "I'm working hard!",
                "icon": "chrome/default.png",
                "pinned": false,
                "protected": true,
                "unique": false,
                "muted": false,
                "url_matcher": null
            }
        },
        {
            "name": "Unique GMail",
            "detection": "STARTS",
            "url_fragment": "https://mail.google.com",
            "tab": {
                "title": null,
                "icon": null,
                "pinned": false,
                "protected": false,
                "unique": true,
                "muted": false,
                "url_matcher": null
            }
        },
        {
            "detection": "CONTAINS",
            "name": "Pinterest search",
            "url_fragment": "pinterest.com/search",
            "tab": {
                "title": "$1 | Pinterest",
                "icon": null,
                "pinned": false,
                "protected": false,
                "unique": false,
                "muted": false,
                "url_matcher": "q=([^&]+)"
            }
        },
        {
            "name": "GitHub",
            "detection": "CONTAINS",
            "url_fragment": "github.com",
            "tab": {
                "title": "{title} | $2 by $1",
                "icon": null,
                "pinned": false,
                "protected": false,
                "unique": false,
                "muted": false,
                "url_matcher": "github[.]com/([A-Za-z0-9_-]+)/([A-Za-z0-9_-]+)"
            }
        }
    ]
}
